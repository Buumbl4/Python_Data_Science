        "def aprovacao(lista):\n",
        "  maior = 0\n",
        "  for elemento in lista:\n",
        "    if elemento > maior:\n",
        "      maior = elemento\n",
        "      menor = maior\n",
        "    if elemento < menor:\n",
        "      menor = elemento\n",
        "  media = round(sum(lista)/len(lista), 1)\n",
        "  if media >= 6:\n",
        "    situacao = 'Aprovado'\n",
        "  else:\n",
        "    situacao = 'Reprovado'\n",
        "\n",
        "  return maior, menor, media, situacao"
      ],
