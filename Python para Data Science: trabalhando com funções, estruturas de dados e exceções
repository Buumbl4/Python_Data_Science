      "cell_type": "code",
      "source": [
        "def leitura(lista):\n",
        "  tamanho_lista = len(lista)\n",
        "  maior = 0\n",
        "  for elemento in lista:\n",
        "    if elemento > maior:\n",
        "      maior = elemento\n",
        "      menor = maior\n",
        "    if elemento < menor:\n",
        "      menor = elemento\n",
        "  soma = sum(lista)\n",
        "\n",
        "  return tamanho_lista, maior, menor, soma"
      ],
      "metadata": {
        "id": "xZJ-87CvypLg"
      },
      "execution_count": 100,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "lista = [16, 14, 63, 65, 17, 99, 70, 11, 20, 48, 79, 32, 17, 89, 12, 25, 66]"
      ],
      "metadata": {
        "id": "d_ppv3xXEKPW"
      },
      "execution_count": 101,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "tam, maior, menor, soma = leitura(lista)"
      ],
      "metadata": {
        "id": "14MEbsLjEOZM"
      },
